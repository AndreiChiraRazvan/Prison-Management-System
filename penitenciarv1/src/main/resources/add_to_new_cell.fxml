<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="600.0" prefWidth="660.0" styleClass="vbox" stylesheets="@background.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.penitenciarv1.Interfaces.popUps.AddToNewCellController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="116.0" prefWidth="639.0">
         <children>
            <Label styleClass="label-title" stylesheets="@background.css" text="Move to another Cell">
               <font>
                  <Font name="Arial" size="24.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" left="20.0" right="20.0" top="40.0" />
               </VBox.margin>
            </Label>
         </children>
         <VBox.margin>
            <Insets left="100.0" right="100.0" />
         </VBox.margin>
      </VBox>
      <VBox alignment="CENTER_LEFT" prefHeight="215.0" prefWidth="0.0">
         <children>
            <Label styleClass="label-text" stylesheets="@background.css" text="Select from available cells:">
               <font>
                  <Font name="Arial" size="18.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="5.0" left="40.0" right="200.0" top="60.0" />
               </VBox.margin>
            </Label>
            <ComboBox fx:id="comboBox" prefHeight="25.0" prefWidth="178.0">
               <VBox.margin>
                  <Insets bottom="20.0" left="40.0" right="350.0" top="5.0" />
               </VBox.margin>
            </ComboBox>
            <Label prefHeight="21.0" prefWidth="180.0" styleClass="label-text" stylesheets="@background.css" text="Reason for transfer:">
               <font>
                  <Font name="Arial" size="18.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="5.0" left="40.0" right="200.0" top="10.0" />
               </VBox.margin>
            </Label>
            <TextArea maxWidth="204.0" prefHeight="54.0" prefWidth="204.0">
               <VBox.margin>
                  <Insets bottom="40.0" left="40.0" right="300.0" top="5.0" />
               </VBox.margin>
            </TextArea>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" right="100.0" />
         </VBox.margin>
      </VBox>
      <VBox alignment="CENTER" prefHeight="185.0" prefWidth="460.0">
         <children>
            <Button fx:id="button" mnemonicParsing="false" onAction="#updateAndExit" prefHeight="45.0" prefWidth="97.0" stylesheets="@background.css" text="Apply">
               <font>
                  <Font size="14.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="40.0" top="40.0" />
               </VBox.margin>
            </Button>
         </children>
         <VBox.margin>
            <Insets bottom="80.0" left="100.0" right="100.0" />
         </VBox.margin>
      </VBox>
   </children>
   <padding>
      <Insets bottom="20.0" />
   </padding>
</VBox>
